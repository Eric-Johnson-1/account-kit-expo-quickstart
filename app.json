{
	"expo": {
		"name": "account-kit-expo-quickstart",
		"slug": "account-kit-expo-quickstart",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "account-kit-expo-quickstart",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.alchemy.accountkitexpoquickstart"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.alchemy.accountkitexpoquickstart"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			["expo-build-properties", { "ios": { "deploymentTarget": "17.0" } }]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
